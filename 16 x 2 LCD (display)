#include <Wire.h>
#include <LiquidCrystal_I2C.h>

// 0x27 or 0x3F (check I2C address if no display)
LiquidCrystal_I2C lcd(0x27, 16, 2); 

void setup() {
  lcd.init();
  lcd.backlight();
  lcd.setCursor(0, 0);
  lcd.print("LCD Test");
  lcd.setCursor(0, 1);
  lcd.print(“Welcome!”);
}

void loop() {
  // Nothing here
}
