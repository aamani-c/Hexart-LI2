int vibrationPin = D5;
int val = 0;

void setup() {
  Serial.begin(9600);
  pinMode(vibrationPin, INPUT);
}

void loop() {
  val = digitalRead(vibrationPin);
  if (val == HIGH) {
    Serial.println("Vibration Detected!");
    delay(200);
  } else {
    Serial.println("No Vibration");
  }
  delay(500);
}
